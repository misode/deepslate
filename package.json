{
  "name": "deepslate",
  "description": "Library for rendering and emulating parts of Minecraft",
  "version": "0.9.0-beta.2",
  "author": "Misode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/misode/deepslate.git"
  },
  "main": "./dist/deepslate.cjs.js",
  "module": "./dist/deepslate.esm.js",
  "unpkg": "./dist/deepslate.umd.js",
  "types": "./dist/deepslate.d.ts",
  "files": [
    "dist/*",
    "!dist/**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "test": "env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register 'test/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "version": "bash scripts/version.sh",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "pako": "^2.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/pako": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "^4.2.0",
    "eslint": "^7.27.0",
    "jszip": "^3.7.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rollup": "^2.56.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vite": "^2.3.4"
  }
}
